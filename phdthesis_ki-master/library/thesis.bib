  keywords = {Gene Expression Regulation,High-Throughput Nucleotide Sequencing,Protein Biosynthesis,Ribosomes,RNA; Messenger,Sequence Analysis; RNA,Software},
  language = {eng},
  number = {1},
  pmcid = {PMC5198522},
  pmid = {27634950}
}

