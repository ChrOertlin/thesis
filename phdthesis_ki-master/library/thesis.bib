@article{Spealman2018,
  title = {Conserved Non-{{AUG uORFs}} Revealed by a Novel Regression Analysis of Ribosome Profiling Data},
  author = {Spealman, Pieter and Naik, Armaghan W. and May, Gemma E. and Kuersten, Scott and Freeberg, Lindsay and Murphy, Robert F. and McManus, Joel},
