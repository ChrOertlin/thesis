  issue = {7347},
  keywords = {Animals,Gene Expression Profiling,Gene Expression Regulation,Half-Life,Mammals,Mice,Models; Genetic,NIH 3T3 Cells,Protein Biosynthesis,Proteins,Reproducibility of Results,RNA; Messenger,Staining and Labeling},
