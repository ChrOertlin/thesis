@article{Schneck2013,
  title = {Analysing the Mutational Status of {{PIK3CA}} in Circulating Tumor Cells from Metastatic Breast Cancer Patients},
  author = {Schneck, Helen and Blassl, Christina and {Meier-Stiegen}, Franziska and Neves, Rui Pedro and Janni, Wolfgang and Fehm, Tanja and Neubauer, Hans},
