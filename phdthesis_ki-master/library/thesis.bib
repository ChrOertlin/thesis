  author = {Hancock, Melissa L. and Meyer, Rebecca C. and Mistry, Meeta and Khetani, Radhika S. and Wagschal, Alexandre and Shin, Taehwan and Ho Sui, Shannan J. and Näär, Anders M. and Flanagan, John G.},
