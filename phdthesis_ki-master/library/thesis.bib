@article{Guan2017,
  title = {A {{Unique ISR Program Determines Cellular Responses}} to {{Chronic Stress}}},
  author = {Guan, Bo-Jhih and van Hoef, Vincent and Jobava, Raul and Elroy-Stein, Orna and Valasek, Leos Shivaya and Cargnello, Marie and Gao, Xing-Huang and Krokowski, Dawid and Merrick, William C. and Kimball, Scot R. and Komar, Anton A. and Koromilas, Antonis E. and Wynshaw-Boris, Anthony and Topisirovic, Ivan and Larsson, Ola and Hatzoglou, Maria},
