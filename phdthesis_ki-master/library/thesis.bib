  author = {Molinaro, Angela and Becattini, Barbara and Mazzoli, Arianna and Bleve, Augusto and Radici, Lucia and Maxvall, Ingela and Sopasakis, Victoria Rotter and Molinaro, Antonio and Bäckhed, Fredrik and Solinas, Giovanni},
  date = {2019-06-04},
  journaltitle = {Cell Metabolism},
