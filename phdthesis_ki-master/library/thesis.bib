@article{Pearce2007,
  title = {Identification of {{Protor}} as a Novel {{Rictor}}-Binding Component of {{mTOR}} Complex-2},
  author = {Pearce, Laura R. and Huang, Xu and Boudeau, Jérôme and Paw\textbackslash lowski, Rafa\textbackslash l and Wullschleger, Stephan and Deak, Maria and Ibrahim, Adel F. M. and Gourlay, Robert and Magnuson, Mark A. and Alessi, Dario R.},
  date = {2007-08},
  journaltitle = {Biochemical Journal},
  volume = {405},
  pages = {513--522},
  issn = {0264-6021},
