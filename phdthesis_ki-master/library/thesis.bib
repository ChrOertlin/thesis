  author = {Puleo, Francesco and Nicolle, Rémy and Blum, Yuna and Cros, Jérôme and Marisa, Laetitia and Demetter, Pieter and Quertinmont, Eric and Svrcek, Magali and Elarouci, Nabila and Iovanna, Juan and Franchimont, Denis and Verset, Laurine and Galdon, Maria Gomez and Devière, Jacques and de Reyniès, Aurélien and Laurent-Puig, Pierre and Van Laethem, Jean-Luc and Bachet, Jean-Baptiste and Maréchal, Raphaël},
