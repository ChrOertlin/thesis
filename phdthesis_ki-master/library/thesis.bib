  author = {Lorent, Julie and Kusnadi, Eric P and van Hoef, Vincent and Rebello, Richard J and Leibovitch, Matthew and Ristau, Johannes and Chen, Shan and Lawrence, Mitchell G and Szkop, Krzysztof J and Samreen, Baila and Balanathan, Preetika and Rapino, Francesca and Close, Pierre and Bukczynska, Patricia and Scharmann, Karin and Takizawa, Itsuhiro and Risbridger, Gail P and Selth, Luke A and Leidel, Sebastian A and Lin, Qishan and Topisirovic, Ivan and Larsson, Ola and Furic, Luc},
  date = {2019-12-02},
  journaltitle = {The EMBO Journal},
