  author = {Ivanov, Alexandr and Mikhailova, Tatyana and Eliseev, Boris and Yeramala, Lahari and Sokolova, Elizaveta and Susorov, Denis and Shuvalov, Alexey and Schaffitzel, Christiane and Alkalaeva, Elena},
  year = {2016},
