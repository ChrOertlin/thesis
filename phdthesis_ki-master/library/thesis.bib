@article{Eraslan2019,
  title = {Quantification and Discovery of Sequence Determinants of Protein-per-{{mRNA}} Amount in~29~Human Tissues},
  author = {Eraslan, Basak and Wang, Dongxue and Gusic, Mirjana and Prokisch, Holger and Hallström, Björn M and Uhlén, Mathias and Asplund, Anna and Pontén, Frederik and Wieland, Thomas and Hopf, Thomas and Hahne, Hannes and Kuster, Bernhard and Gagneur, Julien},
  date = {2019-02-01},
  journaltitle = {Molecular Systems Biology},
