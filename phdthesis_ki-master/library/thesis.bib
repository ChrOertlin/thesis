@article{Jackson1991,
  title = {The {{ATP}} Requirement for Initiation of Eukaryotic Translation Varies According to the {{mRNA}} Species},
  author = {Jackson, Richard J.},
  date = {1991},
  journaltitle = {European Journal of Biochemistry},
