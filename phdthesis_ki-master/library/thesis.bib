@article{Paraskeva1999,
  title = {Ribosomal {{Pausing}} and {{Scanning Arrest}} as {{Mechanisms}} of {{Translational Regulation}} from {{Cap}}-{{Distal}}  {{Iron}}-{{Responsive Elements}}},
  author = {Paraskeva, Efrosyni and Gray, Nicola K. and Schläger, Britta and Wehr, Kristina and Hentze, Matthias W.},
