}

@article{Cheng2016,
  title = {Differential Dynamics of the Mammalian {{mRNA}} and Protein Expression Response to Misfolding Stress},
  author = {Cheng, Zhe and Teo, Guoshou and Krueger, Sabrina and Rock, Tara M. and Koh, Hiromi W. L. and Choi, Hyungwon and Vogel, Christine},
