  author = {Graff, Jeremy R. and Konicek, Bruce W. and Lynch, Rebecca L. and Dumstorf, Chad A. and Dowless, Michele S. and McNulty, Ann M. and Parsons, Stephen H. and Brail, Leslie H. and Colligan, Bruce M. and Koop, Jonathan W. and Hurst, Bernadette M. and Deddens, James A. and Neubauer, Blake L. and Stancato, Louis F. and Carter, Harry W. and Douglass, Larry E. and Carter, Julia H.},
