  eprinttype = {pmid},
  file = {C\:\\Users\\coert\\Zotero\\storage\\FXAIG6IS\\Gingold et al. - 2014 - A Dual Program for Translation Regulation in Cellu.pdf;C\:\\Users\\coert\\Zotero\\storage\\GD4GKABZ\\Gingold et al. - 2014 - A dual program for translation regulation in cellu.pdf},
  keywords = {Anticodon,Cell Differentiation,Cell Line; Tumor,Cell Proliferation,Cell Transformation; Neoplastic,Codon,Histones,Humans,Neoplasms,Protein Biosynthesis,RNA; Messenger,RNA; Transfer,Transcriptome},
  langid = {english},
