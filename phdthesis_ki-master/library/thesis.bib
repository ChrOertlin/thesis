  author = {{Szavits-Nossan}, Juraj and Ciandrini, Luca},
  year = {2020},
  month = sep,
  volume = {48},
  pages = {9478--9490},
  issn = {0305-1048},
  doi = {10.1093/nar/gkaa678},
  abstract = {One of the main goals of ribosome profiling is to quantify the rate of protein synthesis at the level of translation. Here, we develop a method for inferring translation elongation kinetics from ribosome profiling data using recent advances in mathematical modelling of mRNA translation. Our method distinguishes between the elongation rate intrinsic to the ribosome's stepping cycle and the actual elongation rate that takes into account ribosome interference. This distinction allows us to quantify the extent of ribosomal collisions along the transcript and identify individual codons where ribosomal collisions are likely. When examining ribosome profiling in yeast, we observe that translation initiation and elongation are close to their optima and traffic is minimized at the beginning of the transcript to favour ribosome recruitment. However, we find many individual sites of congestion along the mRNAs where the probability of ribosome interference can reach \$50\textbackslash\%\$. Our work provides new measures of translation initiation and elongation efficiencies, emphasizing the importance of rating these two stages of translation separately.},
  file = {C\:\\Users\\ChristianOertlin\\Zotero\\storage\\VXJTDNNL\\Szavits-Nossan and Ciandrini - 2020 - Inferring efficiency of translation initiation and.pdf;C\:\\Users\\ChristianOertlin\\Zotero\\storage\\RV2QLLRH\\5895331.html},
  journal = {Nucleic Acids Research},
  number = {17}
}

