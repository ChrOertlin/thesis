@article{Livingstone2015,
  title = {Assessment of {{mTOR}}-{{Dependent Translational Regulation}} of {{Interferon Stimulated Genes}}},
  author = {Livingstone, Mark and Sikström, Kristina and Robert, Philippe A. and Uzé, Gilles and Larsson, Ola and Pellegrini, Sandra},
  date = {2015-07-24},
  journaltitle = {PLOS ONE},
  shortjournal = {PLOS ONE},
  volume = {10},
