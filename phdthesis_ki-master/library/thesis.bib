  keywords = {Adolescent,Adult,Analysis of Variance,Case-Control Studies,CD4-Positive T-Lymphocytes,Child,Concanavalin A,Depression,Female,Humans,Immunologic Techniques,Leukocyte Count,Longitudinal Studies,Lymphocyte Activation,Male,Psychoneuroimmunology,Regression Analysis,Reproducibility of Results,Stress; Psychological},
  language = {eng},
