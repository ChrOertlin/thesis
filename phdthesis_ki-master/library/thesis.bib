@article{Cheng2013,
  title = {Eukaryotic Initiation Factor {{4B}} and the Poly({{A}})-Binding Protein Bind {{eIF4G}} Competitively},
  author = {Cheng, Shijun and Gallie, Daniel R.},
  year = {2013},
