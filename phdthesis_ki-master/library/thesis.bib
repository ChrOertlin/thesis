@article{Dassi2015,
  ids = {Dassi2015a,Dassi2015b},
  title = {Translational Compensation of Genomic Instability in Neuroblastoma},
  author = {Dassi, Erik and Greco, Valentina and Sidarovich, Viktoryia and Zuccotti, Paola and Arseni, Natalia and Scaruffi, Paola and Tonini, Gian Paolo and Quattrone, Alessandro},
  date = {2015-09-24},
