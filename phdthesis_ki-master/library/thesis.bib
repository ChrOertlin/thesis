}

@article{Guzeloglu-Kayisli2008,
  title = {Identification and Characterization of Human Embryonic Poly({{A}}) Binding Protein ({{EPAB}})},
  author = {Guzeloglu-Kayisli, Ozlem and Pauli, Samuel and Demir, Habibe and Lalioti, Maria D. and Sakkas, Denny and Seli, Emre},
  date = {2008-10-01},
  journaltitle = {Molecular Human Reproduction},
