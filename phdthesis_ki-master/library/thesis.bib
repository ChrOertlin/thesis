@article{Rato2011,
  title = {Translational Recoding as a Feedback Controller: Systems Approaches Reveal Polyamine-Specific Effects on the Antizyme Ribosomal Frameshift},
  shorttitle = {Translational Recoding as a Feedback Controller},
  author = {Rato, Claudia and Amirova, Svetlana R. and Bates, Declan G. and Stansfield, Ian and Wallace, Heather M.},
